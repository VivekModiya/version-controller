{
    "manifest_version": 3,
    "name": "Control Version",
    "description": "This extension changes the version b/w default, next and later",
    "version": "1.0",
    "action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["activeTab", "scripting"]
}
